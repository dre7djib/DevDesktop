<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DevDesktop_CamaraDjibril.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DevDesktop_CamaraDjibril"
    Shell.FlyoutBehavior="Flyout"
    Title="DevDesktop_CamaraDjibril">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Watched Films"
        ContentTemplate="{DataTemplate local:Pages.WatchedFilmsPage}"
        Route="WatchedFilmsPage" />

</Shell>
